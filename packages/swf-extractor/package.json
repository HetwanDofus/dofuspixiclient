{
  "name": "@aspect/swf-extractor",
  "version": "1.0.0",
  "description": "SWF file parser and resource extractor - TypeScript port of ArakneSwf",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "swf-extract": "./bin/swf-extract.ts"
  },
  "scripts": {
    "build": "bun build ./src/index.ts --outdir ./dist --target node",
    "typecheck": "tsc --noEmit",
    "test": "bun test",
    "extract": "bun run ./bin/swf-extract.ts"
  },
  "dependencies": {
    "@napi-rs/canvas": "^0.1.83",
    "@resvg/resvg-js": "^2.6.2",
    "@skyra/gifenc": "^1.0.1",
    "commander": "^14.0.2",
    "sharp": "^0.33.0",
    "ts-pattern": "^5.9.0"
  },
  "devDependencies": {
    "@types/bun": "^1.3.3",
    "@types/node": "^24.10.1",
    "@types/sharp": "^0.32.0",
    "bun-types": "^1.3.3",
    "typescript": "^5.3.0"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  },
  "license": "LGPL-3.0-or-later",
  "author": "Port of ArakneSwf by Vincent Quatrevieux"
}
